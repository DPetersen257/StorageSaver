{
  "manifest_version": 2,
  "name": "Storage Saver",
  "version": "1.0",
  "description": "Save, import, and export local and session storage.",
  "permissions": ["activeTab", "storage", "<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icon.png"
  }
}
